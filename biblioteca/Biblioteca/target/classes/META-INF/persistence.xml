        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>biblioteca.informatie.Articol</class>
        <class>biblioteca.informatie.Autor</class>
        <class>biblioteca.informatie.Carte</class>
        <class>biblioteca.informatie.CuvinteCheie</class>
        <class>biblioteca.informatie.Editura</class>
        <class>biblioteca.informatie.Locatie</class>
